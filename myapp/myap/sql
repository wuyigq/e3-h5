sql


CREATE TABLE IF NOT EXISTS user (
  id INT(11) NOT NULL AUTO_INCREMENT,
  name VARCHAR(45) DEFAULT NULL,
  pwd VARCHAR(200) DEFAULT NULL,
  account_type 
  regist_date DATE DEFAULT NULL,
  login_date DATE DEFAULT NULL,
  vip INT(4) DEFAULT 0,
  phone 

  PRIMARY KEY (id)
) ENGINE=InnoDB;

